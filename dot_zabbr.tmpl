abbr "-"="cd -"
abbr ".."="cd .."
abbr "..."="cd ../.."
abbr "...."="cd ../../.."
abbr "....."="cd ../../../.."
abbr "e"="nvim"
abbr "se"="sudo nvim"
abbr "ta"="tmux attach -t"
abbr "tad"="tmux attach -d -t"
abbr "tkss"="tmux kill-session -t"
abbr "tksv"="tmux kill-server"
abbr "tl"="tmux list-sessions"
abbr "ts"="tmux new-session -s"
{{ if lookPath "fdfind" -}}
abbr "fd"="fdfind"
{{ end -}}
{{ if lookPath "lazydocker" -}}
abbr "lad"="lazydocker"
{{ end -}}
{{ if lookPath "lazygit" -}}
abbr "lag"="lazygit"
{{ end -}}
{{ if lookPath "task" -}}
abbr "t"="task"
{{ end -}}
{{ if lookPath "timew" -}}
abbr "tw"="timew"
{{ end -}}
{{ if lookPath "taskwarrior-tui" -}}
abbr "ti"="taskwarrior-tui"
{{ end -}}
